<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.8.3">
    <id>WildernessLabs.Meadow.Assemblies</id>
    <version>0.8.1-beta05</version>
    <title>Meadow Assemblies</title>
    <authors>Brian Kim, Bryan Costanich, Adrian Stevens</authors>
    <owners>Wilderness Labs</owners>
    <projectUrl>https://wildernesslabs.co</projectUrl>
    <description>Meadow Assemeblies</description>	
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <group targetFramework="netstandard2.1"/>
    </dependencies>
    <contentFiles>
      <files include="**/*.dll" buildAction="None" copyToOutput ="true" flatten="true" /> 
    </contentFiles>
  </metadata>

  <files>
    <file src="contentFiles\**" target="contentFiles" /> 
    <file src="*.dll" target="lib/netstandard2.1"/>
    <file src="*.pdb" target="lib/netstandard2.1"/>
  </files>

</package>